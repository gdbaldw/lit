{
  "name": "lit-sudoku",
  "description": "sandbox",
  "repository": {
    "type": "",
    "url": ""
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean_build": "lerna exec yarn clean_build",
    "watch": "tsc -b packages/* -w",
    "dev": "netlify dev --dir=out/client --functions=out/lambdas",
    "iterate": "yarn watch & yarn dev",
    "deploy": "netlify deploy --prod --dir=out/client --functions=out/lambdas",
    "postinstall": "lerna exec yarn postinstall"
  },
  "dependencies": {
    "lerna": "^3.19.0",
    "netlify-cli": "^2.21.0"
  }
}